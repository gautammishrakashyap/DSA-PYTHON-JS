    "def poss(N):\n",
    "    if N ==2:\n",
    "        return 2\n",
    "    elif N ==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return poss(N-1)+ poss(N-2)\n",
    "    \n",
    "N=10\n",
    "result = poss(N)\n",
    "print(\"no of ways ~\", result)"
   ]
